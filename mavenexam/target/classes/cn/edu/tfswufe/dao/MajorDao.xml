<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.tfswufe.dao.MajorDao">
    <select id="findMajor" resultType="cn.edu.tfswufe.model.Major">
        select * from major
        <where>
            mid = #{mid}
        </where>
    </select>
</mapper>
