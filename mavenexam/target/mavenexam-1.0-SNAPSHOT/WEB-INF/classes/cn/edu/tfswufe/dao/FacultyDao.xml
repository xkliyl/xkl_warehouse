<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.tfswufe.dao.FacultyDao">
    <select id="findFaculty" resultType="cn.edu.tfswufe.model.Faculty">
        select * from faculty
        <where>
            fid = #{fid}
        </where>
    </select>
</mapper>
